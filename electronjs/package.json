{
  "name": "electron-fillr-integration",
  "version": "0.1.1",
  "description": "A minimal Electron fillr application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "build --mac"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fillr/browser-example-integration.git"
  },
  "keywords": [
    "electron-fillr-integration"
  ],
  "author": "Fillr",
  "license": "ISC",
  "devDependencies": {
    "electron": "^5.0.4",
    "electron-builder": "^20.40.2"
  },
  "dependencies": {
    "fillr-extension": "^0.1.3"
  },
  "build": {
    "productName": "Fillr Electron",
    "appId": "com.electron.fillr",
    "asar": true,
    "mac": {
      "target": [
        "default"
      ],
      "icon": "./resources/icon.png"
    },
    "dmg": {
      "title": "electron-fillr-sample",
      "icon": "./resources/icon.png"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources",
      "output": "dist/",
      "app": "."
    }
  }
}
